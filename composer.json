{
    "name": "chr15k/laravel-meilisearch-filter",
    "description": "Laravel Meilisearch filter generator to work with Laravel Scout package",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "laravel",
        "chr15k",
        "search",
        "scout",
        "php",
        "seo",
        "meilisearch"
    ],
    "authors": [
        {
            "name": "Christopher Keller",
            "email": "ctk8501@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/scout": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.5",
        "phpunit/phpunit": "^9.6|^10.5|^11.4"
    },
    "autoload": {
        "psr-4": {
            "Chr15k\\MeilisearchFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chr15k\\MeilisearchFilter\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chr15k\\MeilisearchFilter\\Providers\\MeilisearchFilterServiceProvider"
            ],
            "aliases": {
                "FilterBuilder": "Chr15k\\MeilisearchFilter\\Facades\\FilterBuilder"
            }
        }
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
